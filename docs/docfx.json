{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "Xping.Sdk.Core/Xping.Sdk.Core.csproj",
            "Xping.Sdk.NUnit/Xping.Sdk.NUnit.csproj",
            "Xping.Sdk.XUnit/Xping.Sdk.XUnit.csproj",
            "Xping.Sdk.MSTest/Xping.Sdk.MSTest.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "obj/**",
          "implementation-plans/**",
          "internals/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/media/**"
        ],
        "exclude": [
          "_site/**",
          "obj/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Xping SDK",
      "_appTitle": "Xping SDK for .NET",
      "_appLogoPath": "docs/media/logo.svg",
      "_appFaviconPath": "docs/media/favicon.ico",
      "_appFooter": "<span>Official website <a href=\"https://xping.io\">https://xping.io</a> | Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></span>",
      "_enableSearch": true,
      "_enableNewTab": true,
      "pdf": false
    }
  }
}